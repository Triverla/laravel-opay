{
    "name": "triverla/laravel-opay",
    "description": "A laravel package for Opay",
    "keywords": [
        "php",
        "github",
        "laravel",
        "Open Source",
        "payments",
        "transactions",
        "subscription",
        "opay",
        "opay api",
        "opay.com",
        "laravel opay",
        "laravel-opay",
        "laravel 7",
        "laravel 8",
        "laravel 9",
        "laravel 10"
    ],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Triverla\\LaravelOpay\\": "src"
        }
    },
    "authors": [
        {
            "name": "Benaiah Yusuf",
            "email": "yusufbenaiah@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.2|^8.0|^8.1",
        "illuminate/support":  "~6|~7|~8|~9|^10.0",
        "guzzlehttp/guzzle": "~6|~7|~8|~9",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "1.*",
        "orchestra/testbench": "^5.0|^6.0"
    },
    "autoload-dev": {
        "psr-4": {}
    },
    "scripts": {},
    "extra": {
        "laravel": {
            "providers": [
                "Triverla\\LaravelOpay\\OpayServiceProvider"
            ],
            "aliases": {
                "Opay": "Triverla\\LaravelOpay\\Facades\\Opay"
            }
        }
    }
}
